		- tests/phpunit
	ignoreErrors:
		- message: '/^Call to private\/internal function preferred_languages_load_just_in_time\(\).$/'
	scanFiles:
		- build-cs/vendor/php-stubs/wordpress-tests-stubs/wordpress-tests-stubs.php
